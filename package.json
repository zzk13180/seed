{
  "name": "seed-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/**/*",
    "app/*",
    "crates/*"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "keywords": [
    "sample",
    "seed",
    "monorepo",
    "typescript",
    "tauri",
    "pnpm",
    "lerna",
    "nx",
    "vite",
    "Tauri",
    "Angular",
    "Vue",
    "React",
    "Lit",
    "Astro",
    "poem",
    "prisma",
    "async-graphql",
    "nestjs",
    "husky",
    "commitlint",
    "lint-staged",
    "prettier",
    "eslint",
    "stylelint"
  ],
  "author": "zhangzhankui <zzk13180@gmail.com>",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "prisma": "prisma",
    "dev": "lerna run dev --scope={@seed/web-vue,@seed/app-tauri}",
    "graph": "nx graph",
    "clean": "lerna clean --yes",
    "test": "vitest",
    "prepare": "husky install .githooks",
    "zhlint": "zhlint 'apps/doc/**/*.md'",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "lint:eslint": "eslint --cache --max-warnings 0 \"**/*.{cjs,vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"**/*.{cjs,json,tsx,vue,html,css,scss}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/ --config .stylelintrc.json"
  },
  "dependencies": {
    "@seed/tsconfig": "workspace:*"
  },
  "devDependencies": {
    "lerna": "^6.0.1",
    "nx": "^15.0.5",
    "vitest": "^0.24.5",
    "prisma": "^4.5.0",
    "@prisma/client": "^4.5.0",
    "typescript": "^4.8.4",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@nrwl/cli": "15.0.5",
    "@nrwl/js": "15.0.5",
    "@nrwl/nx-cloud": "^15.0.1",
    "@nrwl/workspace": "15.0.5",
    "@types/codemirror": "^5.60.5",
    "@types/lodash": "^4.14.187",
    "@types/node": "^18.11.9",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.11.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^44.0.2",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "postcss-scss": "^4.0.5",
    "postcss-less": "^6.0.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-order": "^5.0.0",
    "vue-eslint-parser": "^9.1.0",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "zhlint": "^0.6.2"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "resolutions": {
    "rollup": "^2.56.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": []
    }
  }
}
