{
  "name": "seed-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/**",
    "app/**",
    "crates/**"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "keywords": [
    "seed",
    "sample",
    "monorepo",
    "rust",
    "typescript",
    "pnpm",
    "lerna",
    "nx",
    "vite",
    "Tauri",
    "Angular",
    "Vue",
    "React",
    "Lit",
    "Astro",
    "poem",
    "prisma",
    "async-graphql",
    "nestjs",
    "husky",
    "commitlint",
    "lint-staged",
    "prettier",
    "eslint",
    "stylelint"
  ],
  "author": "zhangzhankui <zzk13180@gmail.com>",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "graph": "nx graph",
    "dev:web": "nx dev @seed/web-vue",
    "dev": "lerna run dev --scope={@seed/web-vue,@seed/app-tauri}",
    "commit": "cz",
    "prisma": "prisma",
    "clean": "lerna clean --yes",
    "test": "vitest",
    "prepare": "husky install .githooks",
    "zhlint": "zhlint 'apps/doc/**/*.md'",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "lint:eslint": "eslint --cache --max-warnings 0 \"**/*.{cjs,vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"**/*.{cjs,json,tsx,vue,html,css,scss}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/ --config .stylelintrc.json",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "@seed/tsconfig": "workspace:*",
    "@seed/types": "workspace:*"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@nrwl/workspace": "^15.9.2",
    "@prisma/client": "^4.12.0",
    "@types/codemirror": "^5.60.7",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "commitizen": "^4.3.0",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.17.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^46.0.0",
    "eslint-plugin-vue": "^9.10.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lerna": "^6.6.1",
    "less": "^4.1.3",
    "lint-staged": "^13.2.0",
    "nx": "^15.9.2",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "rimraf": "^4.4.1",
    "sass": "^1.60.0",
    "stylelint": "^15.4.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^11.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-order": "^6.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.2.1",
    "vitest": "^0.29.8",
    "vue-eslint-parser": "^9.1.1",
    "vue-tsc": "^1.2.0",
    "zhlint": "^0.7.1"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "resolutions": {
    "rollup": "^2.56.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [],
      "allowedVersions": {
        "vite": "4"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
