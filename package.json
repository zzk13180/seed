{
  "name": "seed-monorepo",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.10.0",
  "author": "zhangzhankui <zzk13180@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "nx run-many --target=type-check --all",
    "lint": "pnpm lint:eslint && pnpm lint:stylelint && pnpm format",
    "lint:eslint": "eslint --fix .",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,css,scss}\" --allow-empty-input",
    "format": "prettier --write .",
    "nx": "nx",
    "graph": "nx graph",
    "pre-commit": "lint-staged",
    "deps:check": "pnpm --recursive outdated",
    "deps:update": "pnpm --recursive update",
    "deps:dedupe": "pnpm dedupe"
  },
  "dependencies": {
    "@seed/common": "workspace:*",
    "@seed/http": "workspace:*",
    "@seed/tsconfig": "workspace:*",
    "@seed/types": "workspace:*"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@nx/js": "^22.3.3",
    "@nx/nest": "^22.3.3",
    "@nx/vite": "^22.3.3",
    "@types/node": "24.0.4",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "astro-eslint-parser": "^1.2.2",
    "cross-env": "^7.0.3",
    "eslint": "9.29.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.1",
    "eslint-plugin-regexp": "^2.9.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "eslint-plugin-svelte": "^3.9.3",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-vue": "10.2.0",
    "globals": "^16.2.0",
    "lint-staged": "^16.1.2",
    "nx": "21.2.1",
    "postcss": "8.5.6",
    "postcss-scss": "^4.0.9",
    "prettier": "3.6.1",
    "prettier-plugin-svelte": "^3.4.0",
    "sass": "1.89.2",
    "stylelint": "^16.21.0",
    "stylelint-config-recommended": "^16.0.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.12.1",
    "svelte-eslint-parser": "^1.2.0",
    "tailwindcss": "^4.1.10",
    "type-fest": "^5.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vitest": "3.2.4",
    "vue-eslint-parser": "10.1.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --fix --max-warnings=0"
    ],
    "*.vue": [
      "eslint --fix --max-warnings=0",
      "stylelint --fix --allow-empty-input"
    ],
    "*.{css,scss}": [
      "stylelint --fix --allow-empty-input"
    ],
    "*.{json,md,yaml,yml,html}": [
      "prettier --write"
    ],
    "*.svelte": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "bracketSameLine": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "arrowParens": "avoid",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "never",
    "htmlWhitespaceSensitivity": "css",
    "vueIndentScriptAndStyle": true,
    "endOfLine": "auto"
  }
}
