{
  "name": "seed-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/**/*",
    "app/*",
    "configs"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "keywords": [
    "sample",
    "web",
    "tauri",
    "seed",
    "monorepo"
  ],
  "author": "zhangzhankui <zzk13180@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nx dev @seed/web-vue",
    "graph": "nx graph",
    "clean": "lerna clean",
    "test": "vitest",
    "prepare": "husky install .githooks",
    "zhlint": "zhlint 'packages/**/*.md'",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "lint:eslint": "eslint --cache --max-warnings 0 \"**/*.{cjs,vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"**/*.{cjs,json,tsx,vue,html}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/"
  },
  "dependencies": {
    "@seed/configs": "workspace:*",
    "@seed/common": "workspace:*",
    "@angular/animations": "^14.2.8",
    "@angular/common": "^14.2.8",
    "@angular/compiler": "^14.2.8",
    "@angular/core": "^14.2.8",
    "@angular/forms": "^14.2.8",
    "@angular/platform-browser": "^14.2.8",
    "@angular/platform-browser-dynamic": "^14.2.8",
    "@angular/router": "^14.2.8",
    "@angular/language-service": "^14.2.8",
    "@angular/platform-server": "^14.2.8",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8",
    "vue": "^3.2.37",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "lit": "^2.4.0",
    "astro": "^1.5.0",
    "rxjs": "^7.5.6",
    "@astrojs/vue": "^1.1.0",
    "@astrojs/react": "^1.1.0",
    "@tauri-apps/api": "^1.1.0",
    "vue-router": "^4.1.3",
    "pinia": "^2.0.18",
    "pinia-plugin-persist": "^1.0.0",
    "axios": "^0.27.2",
    "qs": "^6.11.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^0.2.0-alpha.12",
    "@angular-devkit/build-angular": "^14.0.3",
    "@angular/cli": "~14.0.3",
    "@angular/compiler-cli": "^14.2.8",
    "vite": "^3.1.8",
    "lerna": "^6.0.1",
    "nx": "^15.0.3",
    "@nrwl/cli": "15.0.3",
    "@nrwl/js": "15.0.3",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "15.0.3",
    "prisma": "^4.5.0",
    "@prisma/client": "^4.5.0",
    "vitest": "^0.21.1",
    "typescript": "^4.8.4",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/lodash": "^4.14.182",
    "@types/codemirror": "^5.60.5",
    "@types/node": "^18.7.1",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/test-utils": "^2.0.2",
    "@babel/core": "^7.19.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.6.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.0",
    "zhlint": "^0.6.2",
    "lint-staged": "^12.4.1",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "jsdom": "^20.0.0",
    "sass": "^1.55.0",
    "less": "^4.1.3",
    "postcss": "^8.4.16",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.10.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^27.0.0",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.9.1",
    "vue-eslint-parser": "^9.0.3",
    "vue-types": "^4.2.1",
    "vite-plugin-windicss": "^1.8.7",
    "windicss": "^3.5.6"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "resolutions": {
    "rollup": "^2.56.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": []
    }
  }
}
