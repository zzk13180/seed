{
  "name": "@seed/custom-ui",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/components/custom-ui/src",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite dev",
        "cwd": "packages/components/custom-ui"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build"],
      "options": {
        "command": "vite build -c= vite.lib.config.js",
        "cwd": "packages/components/custom-ui"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview",
        "cwd": "packages/components/custom-ui"
      }
    }
  },
  "tags": ["type:lib", "framework:svelte"]
}
