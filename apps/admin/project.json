{
  "name": "@seed/admin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/admin/src",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite serve --mode development",
        "cwd": "apps/admin"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build"],
      "options": {
        "command": "vite build --mode production",
        "cwd": "apps/admin"
      },
      "outputs": ["{projectRoot}/dist"],
      "configurations": {
        "development": {
          "command": "vite build --mode development"
        },
        "production": {
          "command": "vite build --mode production"
        }
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "vite preview",
        "cwd": "apps/admin"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vue-tsc --noEmit --skipLibCheck",
        "cwd": "apps/admin"
      }
    }
  },
  "tags": ["type:app", "platform:web"]
}
